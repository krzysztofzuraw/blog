{
  "emmet.includeLanguages": {
    "nunjucks": "html"
  },
  "files.associations": {
    "*.njk": "html"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.validate": false,
  "cSpell.customDictionaries": {
    "blog-words": {
      "name": "blog-words",
      "path": "${workspaceRoot}/.vscode/dict.txt",
      "description": "Words used in this blog",
      "addWords": true
    }
  }
}
