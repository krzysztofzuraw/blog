{
  "name": "krzysztofzuraw.com",
  "version": "3.0.0",
  "description": "Krzysztof Żuraw site",
  "main": "n/a",
  "scripts": {
    "start": "eleventy --serve",
    "prebuild": "npm run clean:dist",
    "build": "eleventy",
    "debug": "DEBUG=* eleventy",
    "clean:dist": "rimraf dist/*",
    "prereinstall": "npm run clean:dist",
    "reinstall": "rimraf node_modules/*",
    "postreinstall": "npm install",
    "img:convert": "npx @squoosh/cli --webp '{quality: 75}'",
    "prepare": "husky install"
  },
  "keywords": [
    "blog",
    "11ty"
  ],
  "author": "Krzysztof Żuraw <info@kzuraw.com> (https://krzysztofzuraw.com/)",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-cache-assets": "2.3.0",
    "@11ty/eleventy-navigation": "0.3.2",
    "@11ty/eleventy-plugin-inclusive-language": "1.0.2",
    "@11ty/eleventy-plugin-rss": "1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.2",
    "dotenv": "10.0.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "lodash.groupby": "4.6.0",
    "luxon": "2.0.1",
    "netlify-plugin-11ty": "1.1.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "sanitize-html": "2.4.0"
  },
  "lint-staged": {
    "*.{js,css,md,html}": "prettier --write"
  }
