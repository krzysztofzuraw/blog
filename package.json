  "main": "n/a",
  "description": "Krzysztof Żuraw homepage",
  "scripts": {
    "dev": "gatsby develop",
    "reinstall": "rimraf node_modules && npm install",
    "build": "gatsby build",
    "prepare": "husky install",
    "lint": "eslint --cache --fix --quiet",
    "prettier": "prettier --write",
    "tsc": "tsc"
