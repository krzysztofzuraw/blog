{
  "name": "blog",
  "version": "4.0.0",
  "description": "Krzysztof Żuraw blog",
  "keywords": [
    "blog",
    "11ty"
  ],
  "license": "MIT",
  "author": "Krzysztof Żuraw (https://krzysztofzuraw.com/)",
  "main": "n/a",
  "scripts": {
    "build": "eleventy",
    "debug": "DEBUG=* eleventy",
    "dev": "eleventy --serve",
    "new:writing": "./scripts/writing.sh",
    "new:reading": "./scripts/reading.sh",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 100
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.2",
    "@11ty/eleventy-img": "2.0.1",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.2.0",
    "luxon": "3.1.1",
    "prettier": "2.8.1",
    "husky": "8.0.0",
    "lint-staged": "13.1.0"
  }
}
