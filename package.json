{
  "name": "krzysztofzuraw.com",
  "version": "4.0.0",
  "description": "Krzysztof Żuraw site",
  "main": "n/a",
  "scripts": {
    "start:eleventy": "eleventy --serve",
    "start:tailwind": "tailwindcss --input ./src/css/index.css --output ./dist/css/index.css --watch",
    "dev": "run-p start:*",
    "prebuild": "npm run clean:dist",
    "build:eleventy": "eleventy",
    "build:tailwind": "NODE_ENV=production tailwindcss  --input ./src/css/index.css --output ./dist/css/index.css --minify",
    "build": "run-s build:*",
    "debug": "DEBUG=* eleventy",
    "clean:dist": "rimraf dist/*",
    "prereinstall": "npm run clean:dist",
    "reinstall": "rimraf node_modules/*",
    "postreinstall": "npm install",
    "img:convert": "./scripts/convert-img.sh"
  },
  "keywords": [
    "blog",
    "11ty",
    "tailwindcss"
  ],
  "author": "Krzysztof Żuraw <info@kzuraw.com> (https://krzysztofzuraw.com/)",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-navigation": "0.3.2",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "image-size": "1.0.0",
    "luxon": "2.3.0",
    "rimraf": "3.0.2",
    "@tailwindcss/typography": "0.5.0",
    "npm-run-all": "4.1.5",
    "tailwindcss": "3.0.11"
  }
}
